{
  "name": "@tscircuit/bun-test-plan",
  "version": "0.0.1",
  "description": "Generate test plans for parallel CI execution with bun test",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "bun-test-plan": "./src/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "files": [
    "src"
  ],
  "keywords": [
    "bun",
    "test",
    "parallel",
    "ci",
    "github-actions",
    "tscircuit"
  ],
  "author": "tscircuit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/bun-test-plan"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
